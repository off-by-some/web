{
  "name": "my-app",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "prebuild": "npm run generate:og",
    "build": "rm -rf build && vite build",
    "postbuild": "cp build/index.html build/404.html && touch build/.nojekyll",
    "deploy": "npm run build && npm run postbuild && gh-pages -d build --dotfiles",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint . && prettier --check .",
    "format": "prettier --write .",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "sass:compile": "sass src/styles/_tokens.scss dist/_tokens.css",
    "sass:watch": "sass --watch src/styles/_tokens.scss:dist/_tokens.css",
    "sass:colors": "sass src/styles/_tokens.scss dist/colors.css",
    "sass:contrast": "sass src/styles/contrast-test.scss dist/contrast-test.css",
    "sass:compile-all": "sass src/styles:dist --no-source-map",
    "generate:og": "node scripts/generate-og-images.js"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.0.1",
    "@eslint/compat": "^1.2.5",
    "@eslint/js": "^9.18.0",
    "@storybook/addon-docs": "^9.0.17",
    "@storybook/addon-svelte-csf": "^5.0.7",
    "@storybook/addon-vitest": "^9.0.17",
    "@storybook/mdx2-csf": "^1.1.0",
    "@storybook/sveltekit": "^9.0.17",
    "@sveltejs/adapter-static": "^3.0.8",
    "@sveltejs/kit": "^2.22.0",
    "@sveltejs/vite-plugin-svelte": "^6.0.0",
    "@types/node": "^20",
    "cssnano": "^7.1.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-storybook": "^9.0.17",
    "eslint-plugin-svelte": "^3.0.0",
    "gh-pages": "^6.3.0",
    "globals": "^16.0.0",
    "mdsvex": "^0.12.6",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.3",
    "sass": "^1.89.2",
    "scss": "^0.2.4",
    "storybook": "^9.0.17",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "svelte-preprocess": "^6.0.3",
    "terser": "^5.43.1",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.20.0",
    "vite": "^7.0.4"
  },
  "dependencies": {
    "@fontsource/inter": "^5.2.6",
    "puppeteer": "^24.16.0",
    "yarn": "^1.22.22"
  }
}
